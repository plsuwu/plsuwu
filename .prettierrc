{
	"experimentalTernaries": true,
	"useTabs": true,
	"singleQuote": false,
	"printWidth": 80,
	"tabWidth": 4,
	"bracketSameLine": false,
	"bracketSpacing": true,
	"trailingComma": "es5",
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss",
		"@trivago/prettier-plugin-sort-imports"
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
